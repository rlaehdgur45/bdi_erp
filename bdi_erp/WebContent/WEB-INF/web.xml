<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>bdi_erp</display-name>
  <welcome-file-list>
    <welcome-file>/WEB-INF/views/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ViewServlet</servlet-name>
    <servlet-class>com.bdi.erp.servlet.ViewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewServlet</servlet-name>
    <url-pattern>/views/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.bdi.erp.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ABCServlet</servlet-name>
    <servlet-class>com.bdi.erp.servlet.ABCServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ABCServlet</servlet-name>
    <url-pattern>/abc/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>MoveServlet</servlet-name>
    <servlet-class>com.bdi.erp.servlet.MoveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MoveServlet</servlet-name>
    <url-pattern>/move/*</url-pattern>
  </servlet-mapping>
  
  
</web-app>